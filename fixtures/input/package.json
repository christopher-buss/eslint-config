{
    "version": "1.0.0",
    "name": "@test/fixture-package",
    "description": "A fixture package for testing",
    "license": "MIT",
    "scripts": {
        "test:watch": "vitest",
        "test": "vitest run",
        "Build": "tsup",
        "lint": "eslint .",
        "dev": "tsup --watch",
        "typeCheck": "tsc --noEmit",
        "clean": "rimraf dist"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "keywords": ["test", "fixture"],
    "repository": {
        "type": "git",
        "url": "https://github.com/test/fixture"
    },
    "homepage": "https://github.com/test/fixture#readme",
    "bugs": {
        "url": "https://github.com/test/fixture/issues"
    },
    "files": ["dist"],
    "engines": {
        "node": ">=18"
    }
}
