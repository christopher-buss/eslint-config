{
	"name": "@isentinel/eslint-config",
	"type": "module",
	"version": "0.3.1",
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@9.0.5",
	"description": "iSentinel's ESLint config",
	"author": "Christopher Buss <christopher.buss@pm.me> (https://github.com/christopher-buss)",
	"contributors": [
		"Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)"
	],
	"license": "MIT",
	"homepage": "https://github.com/christopher-buss/roblox-ts-eslint-config",
	"keywords": [
		"eslint-config",
		"roblox"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": "./bin/index.js",
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "nr typegen && tsup --format esm,cjs --clean --dts",
		"stub": "tsup --format esm",
		"dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
		"watch": "tsup --format esm,cjs --watch",
		"lint": "eslint .",
		"typegen": "esno scripts/typegen.ts",
		"prepack": "nr build",
		"release": "bumpp && pnpm publish --p",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"prepare": "simple-git-hooks"
	},
	"peerDependencies": {
		"@eslint-react/eslint-plugin": "^1.5.8",
		"eslint": ">=8.40.0",
		"eslint-plugin-react-hooks": "^4.6.0"
	},
	"peerDependenciesMeta": {
		"@eslint-react/eslint-plugin": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/eslint-define-config": "1.23.0-2",
		"@antfu/install-pkg": "^0.3.3",
		"@eslint-types/jsdoc": "48.2.2",
		"@eslint-types/unicorn": "^52.0.0",
		"@shopify/eslint-plugin": "^44.0.0",
		"@stylistic/eslint-plugin": "^1.8.0",
		"@typescript-eslint/eslint-plugin": "~7.7.1",
		"@typescript-eslint/parser": "~7.7.1",
		"eslint-config-flat-gitignore": "^0.1.5",
		"eslint-config-prettier": "^9.1.0",
		"eslint-flat-config-utils": "^0.2.4",
		"eslint-plugin-antfu": "^2.1.2",
		"eslint-plugin-arrow-return-style": "^1.3.0",
		"eslint-plugin-comment-length": "^1.7.3",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-format": "^0.1.1",
		"eslint-plugin-import-x": "^0.5.0",
		"eslint-plugin-jsdoc": "^48.2.3",
		"eslint-plugin-jsonc": "^2.15.1",
		"eslint-plugin-markdown": "^4.0.1",
		"eslint-plugin-no-autofix": "^2.0.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^2.10.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-roblox-ts": "^0.0.36",
		"eslint-plugin-sonarjs": "^0.25.1",
		"eslint-plugin-unicorn": "^52.0.0",
		"eslint-plugin-unused-imports": "^3.2.0",
		"eslint-plugin-yml": "^1.14.0",
		"globals": "^15.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^0.5.0",
		"parse-gitignore": "^2.0.0",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prompts": "^2.4.2",
		"yaml-eslint-parser": "^1.2.2",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.12",
		"@eslint-react/eslint-plugin": "^1.5.10",
		"@isentinel/eslint-config": "workspace:*",
		"@stylistic/eslint-plugin-migrate": "^1.8.0",
		"@types/eslint": "^8.56.10",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.12.8",
		"@types/prompts": "^2.4.9",
		"@types/yargs": "^17.0.32",
		"bumpp": "^9.4.1",
		"bundle-require": "^4.0.3",
		"eslint": "^9.1.1",
		"eslint-flat-config-viewer": "^0.1.20",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-typegen": "^0.2.4",
		"esno": "^4.7.0",
		"execa": "^8.0.1",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.2.0",
		"lint-staged": "^15.2.2",
		"rimraf": "^5.0.5",
		"simple-git-hooks": "^2.11.1",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vitest": "^1.5.3"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": "eslint --fix"
	}
}
